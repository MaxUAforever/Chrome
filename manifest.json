{
	"name": "Ginger",
	"description": "The best extension you have ever seen",
	"version": "1.0",
	"icons": { 
		"16": "./img/icon16.png",
		"32": "./img/icon32.png",
        "48": "./img/icon48.png",
        "128": "./img/icon128.png"
    },
	"permissions": [
  		"http://www.google.com/",
  		"https://www.google.com/"
	],
	"manifest_version": 2,
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["jquery-3.4.1.min.js", "script.js"],
			"run_at": "document_end"
		}
	]
}