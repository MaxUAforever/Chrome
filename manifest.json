{
	"name": "Ginger",
	"description": "The best extension you have ever seen",
	"version": "0.3",
	"icons": { 
		"16": "./img/icon16.png",
		"32": "./img/icon32.png",
        "48": "./img/icon48.png",
        "64": "./img/icon64.png",
        "128": "./img/icon128.png"
    },
	"permissions": [
		"activeTab",
  		"https://api.flickr.com/services/rest/"
	],
	"manifest_version": 2,
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"css": ["style.css"],
			"js": ["jquery-3.4.1.min.js", "script.js"],
			"run_at": "document_start"
		}
	],
	"web_accessible_resources": [
		"img/gingerPopup.png"
	]
}
